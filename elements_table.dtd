<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT atomic_number (#PCDATA)>
<!ELEMENT atomic_mass (#PCDATA)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT electrons_on_layer (#PCDATA)>
<!ELEMENT symbol (#PCDATA)>
<!ELEMENT kind (#PCDATA)>
<!ELEMENT group (#PCDATA)>
